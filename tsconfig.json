{
  "compilerOptions": {
    "baseUrl": "src",
    "allowJs": true,
    "paths": {
      "@components/*": ["components/*"],
      "@navigation/*":["navigation/*"],
      "@views/*": ["views/*"], 
      "@assets/*": ["assets/*"],
      "@api/*": ["api/*"],
      "@helpers/*": ["helpers/*"],
      "@store/*": ["store/*"],
      "@services/*": ["services/*"],
      "@models/*": ["models/*"],
      "@hooks/*": ["hooks/*"],
      "@viewmodels/*": ["viewmodels/*"]
    }
  },
  "extends": "@react-native/typescript-config/tsconfig.json"
}
